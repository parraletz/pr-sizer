{
  "name": "add-reviewers-action",
  "version": "1.2.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm i && npm run build",
    "build": "npm run validate-typescript && npm run build:clean && npm run generate-dist",
    "build:docker": "npm run validate-typescript && npm run build:clean && npm run generate-dist:docker",
    "test": "rimraf coverage && npm run test:unit",
    "test:unit": "vitest run --coverage",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "lint:yaml": "chmod +x scripts/lint_yaml.sh && ./scripts/lint_yaml.sh",
    "validate-typescript": "tsc -p tsconfig.json --noEmit",
    "generate-dist": "ncc build src/index.ts",
    "generate-dist:docker": "ncc build src/docker.ts",
    "build:clean": "rimraf dist; exit 0",
    "prepare": "node .husky/install.mjs",
    "typos": "chmod +x scripts/check_typos.sh && ./scripts/check_typos.sh",
    "semantic-release": "semantic-release"
  },
  "keywords": [],
  "author": "parraletz@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@codedependant/semantic-release-docker": "^5.0.3",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-angular": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@eclass/semantic-release-docker": "^4.0.0",
    "@semantic-release-plus/docker": "^3.1.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.4",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.10",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "regenerator-runtime": "^0.14.1",
    "replace-json-property": "^1.9.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.4.0",
    "vitest": "^2.0.5",
    "vitest-mock-extended": "^2.0.0"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/rest": "^21.0.1",
    "js-yaml": "^4.1.0"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  }
}
